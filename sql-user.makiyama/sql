■問1
(13,1,'鰯',150),
(14,2,'羊',650);

■問2
UPDATE items SET price = price * 0.9;

■問3
DELETE FROM employees WHERE end_date <= '2013-3-31';

■問4
SELECT name, email, start_date, end_date
FROM employees
WHERE end_date IS NULL
ORDER BY start_date ASC
LIMIT 1;

■問5
SELECT
r.name,p.name
FROM
regions AS r
INNER JOIN
prefectures AS p
ON
r.code=p.region_code;

■問6
SELECT SUM(population) FROM populations;

■問7
SELECT 
p.name AS prefecture, 
g.name AS gender, 
SUM(po.population) AS population
FROM populations AS po
INNER JOIN prefectures AS p ON po.prefecture_code = p.code
INNER JOIN genders AS g ON po.gender_code = g.code
WHERE p.name IN ('福岡県', '佐賀県', '長崎県', '熊本県', '大分県', '宮崎県', '鹿児島県', '沖縄県')
GROUP BY p.name, g.name, p.code, g.code
ORDER BY p.code, g.code DESC;

